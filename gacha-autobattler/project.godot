; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GachaAutobattler"
config/version="0.6"
run/main_scene="res://scenes/ui/main_menu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

PlayerData="*res://scripts/core/player_data.gd"
AchievementManager="*res://scripts/core/achievement_manager.gd"
NetworkManager="*res://scripts/autoload/network_manager.gd"
UITheme="*res://scripts/core/ui_theme.gd"
UnitSpriteLoader="*res://scripts/core/unit_sprite_loader.gd"
BoardAssetLoader="*res://scripts/core/board_asset_loader.gd"
SceneTransition="*res://scripts/core/scene_transition.gd"
AudioManager="*res://scripts/core/audio_manager.gd"
TutorialManager="*res://scripts/core/tutorial_manager.gd"
UISpriteLoader="*res://scripts/core/ui_sprite_loader.gd"
CheatManager="*res://scripts/core/cheat_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
